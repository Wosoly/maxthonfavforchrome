{
  "name": "Maxthon Online Favorites",
  "version": "0.4",
  "manifest_version": 2,
  "description": "Display Maxthon Online Favorites in a popup.",
  "icons": { "128": "128.png",
			 "48" : "48.png"},
  "browser_action": {
    "default_title": "Maxthon Online Favorites",
    "default_icon": "favicon.ico",
    "default_popup": "fav.html"
  },
  "permissions": [
    "tabs",
    "http://fav.maxthon.cn/api/fav/*"
  ]
}
